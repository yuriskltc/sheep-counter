node {
    def server
        stage ('Artifactory configuration') {
            server = Artifactory.server 'JFROGTST'
       }
       stage ('Download') {
           def downloadSpec =
            '''{
                "files": [
                    {
                        "pattern": "my-libs-release-local/sheep-counter-1.0.${BUILDNUM}*",
                        "target": "/opt/sheepcounter/"
                    
                    }
                ]
            }'''
            server.download spec: downloadSpec           
        }
        stage('App start'){
            sh 'java -jar /opt/sheepcounter/sheep-counter-1.0.${BUILDNUM}-SNAPSHOT.jar Base'
        }        
}
